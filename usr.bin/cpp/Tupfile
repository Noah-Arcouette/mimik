include_rules

# toolchain build
: foreach *.c ^test.c |> !host_cc   |> ./obj/%B.o {obj}
: {obj}               |> !host_ccld |> $(ROOT)/toolchain/cpp | $(ROOT)/<toolchain>

: test.c | $(ROOT)/<toolchain> |> $(VALGRIND) $(ROOT)/toolchain/cpp -DNOT_DEFINED -DDEFINED -UNOT_DEFINED -DALSO_DEFINED < %f | nl -ba |>
